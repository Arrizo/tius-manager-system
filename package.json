{
  "name": "tius",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "server:dev": "vite --mode dev",
    "server:proxy": "vite --mode proxy",
    "build": "vite build",
    "build:electron":"rimraf dist && vite build && electron-builder",
    "preview": "vite preview",
    "server:ese": "node ./config/ese.js --watch"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/puppeteer": "^7.0.4",
    "axios": "^1.4.0",
    "commitizen": "4.3.0",
    "crypto-js": "^4.1.1",
    "cz-conventional-changelog": "3.3.0",
    "element-plus": "^2.3.4",
    "husky": "^8.0.3",
    "init": "0.1.2",
    "js-base64": "^3.7.5",
    "jsencrypt": "^3.3.2",
    "mock": "^0.1.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "puppeteer": "^20.7.3",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "spark-md5": "^3.0.2",
    "three": "^0.155.0",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.46.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.0.0",
    "electron": "^26.2.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.0",
    "vue-tsc": "^1.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.ts"
    }
  },
"build": {
    "appId": "com.servermonitor.desktop",
    "productName": "czc",
    "asar": true,
    "copyright": "Copyright © 2023 czc",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "./dist",
      "./package.json",
      "./dist-electron"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080"
      }
    ],
    "releaseInfo": {
      "releaseNotes": "发布版本"
    }
  }
}
